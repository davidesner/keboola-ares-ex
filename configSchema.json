                    "obec",
                    "okres",
                    "mestskaCast",
                    "stat",
                    "ulice",
                    "psc",
                    "zahrPsc",
                    "kodOblasti",
                    "kodKraje",
                    "kodOkresu",
                    "kodObce",
                    "kodPrazskehoObvodu",
                    "kodNobvod",
                    "kodCastiObce",
                    "kodMestskeCasti",
                    "kodUlice",
                    "kodAdresy",
                    "kodObjektu",
                    "pocetPrac",
                    "zanikl",
                    "platceDPH",
                    "datumVzniku",
                    "pravniForma",
                    "dic",
                    "nace",
                    "oboryCinnosti"
                ]
            },
            "minItems": 0,
            "uniqueItems": true
        }
    },
    "required": [
        "aresColumns"
    ]
}
